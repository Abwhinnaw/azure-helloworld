{
  "family": "helloworldtask",
  "networkMode": "awsvpc",
  "executionRoleArn": "ecsTaskExecutionRole",
  "containerDefinitions": [{
    "name": "helloworld-app",
    "image": "706949302588.dkr.ecr.ap-southeast-2.amazonaws.com/testworld:#{Build.BuildId}#",
    "portMappings": [{
      "containerPort": 8080,
      "hostPort": 8080,
      "protocol": "tcp"
    }],
    "essential": true
  }],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}